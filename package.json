{
    "name": "language-kinetic-krl",
    "displayName": "language-kinetic-krl",
    "description": "A KRL language package for VS Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "krl",
            "aliases": ["KRL", "krl"],
            "extensions": [".krl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "krl",
            "scopeName": "source.krl",
            "path": "./syntaxes/krl.tmLanguage.json"
        }]
    }
}
